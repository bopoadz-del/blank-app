      "z1": {"type": "float", "unit": "m", "description": "Elevation at point 1", "min_value": 0, "max_value": 100}
    },
    "output_parameters": {
      "total_head": {"type": "float", "unit": "Pa", "description": "Total head at point 1", "min_value": 0, "max_value": 2000000}
