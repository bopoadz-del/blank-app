    },
    "output_parameters": {
      "slump_flow": {"type": "float", "unit": "mm", "description": "Predicted slump flow", "min_value": 50, "max_value": 800}
    },
    "source": "empirical",
    "source_reference": "Concrete Mix Design - Mehta, Monteiro"
  },
  {
