      "h": {"type": "float", "unit": "m", "description": "Plate thickness", "min_value": 0.005, "max_value": 0.1}
    },
    "output_parameters": {
      "deflection": {"type": "float", "unit": "m", "description": "Maximum deflection", "min_value": 0, "max_value": 0.1}
    },
    "source": "ISO_standard",
    "source_reference": "Theory of Plates and Shells - Timoshenko"
  },
  {
